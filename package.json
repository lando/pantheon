{
  "name": "@lando/pantheon",
  "description": "A Lando plugin that provides a tight integration with Pantheon.",
  "version": "1.7.1",
  "author": "Mike Pirog @pirog",
  "license": "GPL-3.0",
  "repository": "lando/pantheon",
  "bugs": "https://github.com/lando/pantheon/issues/new/choose",
  "homepage": "https://github.com/lando/pantheon",
  "keywords": [
    "lando",
    "lando-plugin",
    "lando-recipe",
    "lando-hosting-integration",
    "pantheon"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "lando": {},
  "main": "index.js",
  "nyc": {
    "include": [
      "lib/**/*.js",
      "recipes/**/*.js",
      "services/**/*.js",
      "types/**/*.js"
    ],
    "exclude": [
      "test/**"
    ],
    "cache": true,
    "all": true
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "docs:build": "LANDO_MVB_VERSION=$(git describe --tags --always --abbrev=1 --match=\"v[0-9].*\") vitepress build docs",
    "docs:dev": "LANDO_MVB_VERSION=$(git describe --tags --always --abbrev=1 --match=\"v[0-9].*\") vitepress dev docs",
    "docs:mvb": "npx mvb docs",
    "docs:preview": "vitepress preview docs",
    "lint": "eslint . --ext .js --ext .mjs ",
    "test:unit": "nyc --reporter=html --reporter=text mocha --timeout 5000 test/**/*.spec.js",
    "test:leia": "leia \"examples/**/README.md\" -c 'Destroy tests' --stdin",
    "test": "npm run lint && npm run test:unit"
  },
  "dependencies": {
    "@lando/mariadb": "^1.5.0",
    "@lando/mssql": "^1.0.0",
    "@lando/mysql": "^1.1.0",
    "@lando/php": "^1.5.0",
    "@lando/postgres": "^1.1.0",
    "@lando/redis": "^1.0.0",
    "@lando/solr": "^1.0.0",
    "@lando/varnish": "^1.0.0",
    "axios": "^0.24.0",
    "bluebird": "^3.7.2",
    "dayjs": "^1.10.7",
    "js-yaml": "^3.4.6",
    "lodash": "^4.17.21",
    "winston": "2.4.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.0",
    "@lando/leia": "^1.0.0-beta.3",
    "@lando/vitepress-theme-default-plus": "^1.1.0-beta.14",
    "chai": "^4.3.4",
    "command-line-test": "^1.0.10",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-vue": "^8.0.3",
    "mocha": "^9.1.2",
    "nyc": "^15.1.0",
    "vitepress": "^1.3.4"
  }
}
